#include <stdio.h>
#include <stdlib.h>
int cmp(const void*a,const void*b){return (*(int*)a)-(*(int*)b);}
int main(){
    int arr[]={900,940,950,1100,1500,1800};
    int dep[]={910,1200,1120,1130,1900,2000};
    int n=6; qsort(arr,n,sizeof(int),cmp); qsort(dep,n,sizeof(int),cmp);
    int i=1,j=0,plat=1,res=1;
    while(i<n && j<n){
        if(arr[i]<=dep[j]){plat++;if(plat>res)res=plat;i++;}
        else {plat--;j++;}
    }
    printf("%d\n",res);
}

#include <stdio.h>
#include <stdlib.h>
typedef struct{char id;int d,p;}Job;
int cmp(const void*a,const void*b){return ((Job*)b)->p-((Job*)a)->p;}
int main(){
    Job j[]={{'a',2,100},{'b',1,19},{'c',2,27},{'d',1,25},{'e',3,15}};
    int n=5,slot[10]={0},res[10]; qsort(j,n,sizeof(Job),cmp);
    for(int i=0;i<n;i++){
        for(int k=j[i].d;k>0;k--)
            if(!slot[k]){slot[k]=1;res[k]=i;break;}
    }
    for(int i=1;i<=3;i++) if(slot[i]) printf("%c ",j[res[i]].id);
}
